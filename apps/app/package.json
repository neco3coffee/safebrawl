{
	"name": "@safebrawl/app",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev --turbopack -p 3333",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy:production": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env production",
		"deploy:development": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env development",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.14.4",
		"@radix-ui/react-slot": "^1.2.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.562.0",
		"next": "^15.5.11",
		"next-intl": "^4.6.1",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"sass": "^1.97.1",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.27",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"wrangler": "^4.56.0"
	}
}
